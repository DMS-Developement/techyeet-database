<% content_for :head do %>
  <%= stylesheet_link_tag 'jobseeker-minimal', media: 'all', 'data-turbolinks-track': 'reload' %>
<% end %>

<p style="color: green"><%= notice %></p>

<h1>Job seekers</h1>

<div id="job_seekers">
  <% @job_seekers.each do |job_seeker| %>
    <%= render job_seeker %>
    <p>
      <%= link_to "Show this job seeker", job_seeker %>
    </p>
  <% end %>
</div>

<%= link_to "New job seeker", new_job_seeker_path %>
